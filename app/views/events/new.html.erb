<%= form_with model: @event do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.label :location %>
    <%= f.text_field :location %>
    <%= f.label :price %>
    <%= f.number_field :price %>
    <%= f.label :duration %>
    <%= f.number_field :duration %>
    <%= f.label :start_date %>
    <%= f.datetime_local_field :start_date %>
    <%= f.submit %>
<% end%>